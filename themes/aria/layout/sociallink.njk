{% block sociallink %}
<div class="social-links sidebar-item">
  <p><i class="far fa-address-card"></i> {{ __("sociallink") }}</p>
  <ul>
    {% for k, v in theme.social %}
    <li>{% if v.icon %}<i class="{{ v.icon }}"></i> {% endif %}<a href="{{ v.link }}" target="_blank">{{ k }}</a></li>
    {% endfor %}
  </ul>
</div>
{% endblock %}
